		win_navigation
		{		
			label = "win";

			bindings = <

				// ----------------------------------------

				&kp ESC
				&kp LC(HOME)				// @Home Sprung
				&kp LS(TAB)
				&kp UP						// @Up
				&kp TAB
				&kp LC(LA(LS(C)))	

				&kp LC(LA(C))	
				&kp LC(LA(L))			
				&kp PG_UP
				&kp LC(LA(R))		 	 
				&kp LC(UP)
				&kp BSPC					

				// ----------------------------------------

				&kp LC(LA(B))	
				&kp LC(END)					// @End Sprung
				&kp LEFT 					// @Left
				&kp DOWN					// @Down
				&kp RIGHT 					// @Right
				&kp LC(LA(G))	

				&kp LC(LS(F10))
				&kp LS(LC(TAB))			 	// 
				&kp PG_DN
				&kp LC(TAB)				 	// 
				&kp LC(DOWN)
				&kp ENTER					// -
				
				// ----------------------------------------

				&kp LSHFT
				&kp HOME					// @Home 
				&kp LC(LEFT)				// @Wort Sprung
				&kp LC(RIGHT)				// @Wort Sprung
				&kp END
				&kp DEL

				&wAltShot
				&kp LS(HOME)					// @Home 
				&kp LS(LC(LEFT))				// @Wort Sprung
				&kp LS(LC(RIGHT))				// @Wort Sprung
				&kp LS(END)
				&kp LSHFT

				// ----------------------------------------

				&sl wSYM
				&kp SPACE				
				&sl wFUNC
				&sl wPRG 
				&mdResWin
				&mdNrWin

				// ----------------------------------------							
			>;
		};



